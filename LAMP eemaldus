# Eemalda phpMyAdmin
sudo apt-get remove --purge phpmyadmin
# Eemalda phpMyAdmini seadistusfailid
sudo rm -rf /etc/phpmyadmin
# Eemalda MySQL
sudo apt-get remove --purge mysql-server
# Eemalda MySQL-i seadistusfailid
sudo rm -rf /etc/mysql
# Eemalda PHP
sudo apt-get remove --purge php
# Eemalda Apache veebiserver
sudo apt-get remove --purge apache2
# Eemalda Apache seadistus failid
sudo rm -rf /etc/apache2
# Kustuta kõik veebiserveri (Apache) loodud failid ja kaustad
sudo rm -rf /var/www/html/*
# Värskenda pakettide nimekirja ja eemalda üleliigsed paketid
sudo apt-get update && sudo apt-get autoremove && sudo apt-get autoclean
